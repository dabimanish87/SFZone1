<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.sfzone">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="com.android.hardware.camera" />
    <uses-permission android:name="com.android.hardware.camera.autofocus" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".GruhUdhyog"></activity>
        <activity android:name=".AttendanceReport" />
        <activity android:name=".ScanActivity" />
        <activity android:name=".NotesActivity" />
        <activity android:name=".notes_webview" />
        <activity android:name=".TimeTableActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity
            android:name=".ActivityforHomeScreen"
            android:label="@string/title_activity_activityfor_home_screen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login2Activity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.journeyapps.barcodescanner.CaptureActivity"
            android:screenOrientation="portrait"
            tools:replace="screenOrientation" />
        <activity android:name=".EventActivity" />
    </application>

</manifest>